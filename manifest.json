{
  "manifest_version": 3,
  "name": "Password Manager",
  "description": "Generate secure passwords with a click!",
  "version": "1.0",
  "action": {
    "default_popup": "popup.html", 
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
        "matches": ["<all_urls>"],
        "js": ["password.js"]
    }
  ]
}